  "description": "Adds Platform Indicators for to for users to various places.",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "replugged build plugin",
    "watch": "replugged build plugin --watch",
